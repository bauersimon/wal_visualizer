(defun day15 [end]
  (define spoken (array ("16" 1) ("11" 2) ("15" 3) ("0" 4) ("1" 5)))
  (define now 7)
  (define prev 7)
  (define turn 6)
  
  (while (<= turn end)
    (set (prev now))
    (set (now (if (in prev spoken)
	     	  (- turn (geta spoken prev))
		  0)))
    (seta spoken prev turn)
    (set (turn (+ turn 1))))
  prev)

(print "Advent of Code day 15: " (day15 2020))
