;; In this example we create a fully virtual trace and print it
(new-trace t 30)

(defsig clk (- 1 clk@-1))
(defsig cnt (cond [(! clk) cnt@-1]
                  [(= cnt@-1 3) 0]
                  [#t (+ cnt@-1 1)]))
(defsig overflow (if (= cnt 3) 1 0))                  

;; render the virtual trace
(require print-vcd)
(render '("clk" "cnt" "overflow"))

;; dump the trace to "t.vcd", experimental
(dump-trace t)